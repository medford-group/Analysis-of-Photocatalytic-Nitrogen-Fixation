journal = {Surf. Sci.},
keywords = {Ab initio quantum chemical methods and calculation,Alcohols,Chemisorption,Density functional calculations,Low index single crystal surfaces,Physical adsorption,Single crystal surfaces,Titanium oxide},
number = {2},
pages = {336--349},
pmid = {74906200021},
title = {{The adsorption and dissociation of ROH molecules on TiO2(110)}},
url = {http://www.sciencedirect.com/science/article/pii/S0039602898002787},
volume = {409},
